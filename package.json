{
  "name": "universal-navigation",
  "version": "1.0.0",
  "description": "A small Helper to make navigation between pages writen in different frameworks easy",
  "main": "index.js",
  "scripts": {
    "test": "browserify test.js | browser-run",
    "test:watch": "nodemon -q -x npm test",
    "build:example": "node_modules/.bin/webpack",
    "build:watch": "nodemon -q -x npm run build:example --ignore example/dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcosBessega/Universal-Navigation.git"
  },
