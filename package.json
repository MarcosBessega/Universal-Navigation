  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarcosBessega/Universal-Navigation/issues"
  },
  "homepage": "https://github.com/MarcosBessega/Universal-Navigation#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-run": "^3.3.0",
    "browserify": "^13.1.1",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "style-loader": "^0.13.1",
    "tape": "^4.6.3",
    "vue": "^2.1.8",
